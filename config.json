{
  "dns": {
    "servers": [
      {
        "tag": "google",
        "type": "tls",
        "server": "8.8.8.8"
      },
      {
        "tag": "local",
        "type": "udp",
        "server": "1.1.1.1"
      }
    ],
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "tun",
      "address": ["172.19.0.1/30"],
      "auto_route": true,      
      "strict_route": true
    }
  ],
  "outbounds": [   
	{
      "type": "socks",
      "tag": "socks-out",
      "server": "huynh0201.io.vn",
      "server_port": 12001,
      "username": "huynh0201",
      "password": "Huynh@0201"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "socks-out"
      }
    ],
	"rule_set": [
  		{
	    "tag": "vn-ip",
	    "type": "remote",
	    "format": "text",
	    "url": "https://raw.githubusercontent.com/huynh0201/home-proxy/main/vn-ip.txt"
 		 }
	],
    "default_domain_resolver": "local",
    "auto_detect_interface": true
  }
}
